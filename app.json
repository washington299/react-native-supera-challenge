{
  "expo": {
    "name": "react-native-supera-challenge",
    "slug": "react-native-supera-challenge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/up.png",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.supera.reactnativetest",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/up.png"
    }
  }
}
